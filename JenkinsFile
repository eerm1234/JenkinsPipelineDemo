pipeline {
    agent any
    stages {
        stage('Hello World') {
            steps {
                echo "Hello world"
            }
        }
        stage('Build') {
            steps {
                echo "Building"
            }
        }
        stage('Deploy') {
            steps {
                echo "Deployment"
            }
        }
        stage('Test') {
            steps {
                echo "Testing"
            }
        }
        stage ('Release')  {
            steps {
                echo "Releasing"
            }
        }
    }
}
